#include "${CPP_NAMESPACE}/git.h"

namespace ${CPP_NAMESPACE}
{
    namespace git
    {
        bool dirty()
        {
            return ${IS_DIRTY};
        }

        std::string sha1()
        {
            return std::string("${GIT_SHA1}");
        }

        std::string branch()
        {
            return std::string("${CURRENT_BRANCH_NAME}");
        }
    }
}
